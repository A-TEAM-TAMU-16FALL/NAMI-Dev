%table#recentlyreport-table.table
  %thead
    %tr
      %th Title
      %th Description
      %th Last Modified By
  %tbody
    - if @report_records
      - @report_records.each do |report_record|
        %tr
          %td=report_record.title
          %td=report_record.description
          %td=report_record.last_modified_by
        
= js_will_paginate @report_records